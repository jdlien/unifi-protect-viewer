{
  "name": "unifi-protect-viewer",
  "productName": "UniFi Protect Viewer",
  "version": "1.1.10",
  "description": "Electron app for UniFi Protect",
  "main": "main.js",
  "scripts": {
    "start": "electron --ignore=builds .",
    "dev": "NODE_ENV=development electron .",
    "clean": "rm -rf builds dist || true",
    "build": "electron-builder --config",
    "build:mac": "electron-builder --mac --universal",
    "build:mac-arm64": "electron-builder --mac --arm64",
    "build:mac-x64": "electron-builder --mac --x64",
    "build:mac-notimestamp": "CSC_IDENTITY_AUTO_DISCOVERY=false CSC_DISABLE_TIMESTAMP=true APPLE_TEAM_ID=A93Q7MKECL electron-builder --mac --universal",
    "build:mac-test": "node scripts/build-mac.js",
    "build:win-arm64": "electron-builder --win --arm64",
    "build:win-x64": "electron-builder --win --x64",
    "build:linux": "electron-builder --linux --x64",
    "build:linux-arm64": "electron-builder --linux --arm64",
    "dist": "CSC_IDENTITY_AUTO_DISCOVERY=false pnpm clean && electron-builder --mac --win --publish never",
    "dist:mac": "CSC_IDENTITY_AUTO_DISCOVERY=false pnpm clean && electron-builder --mac --publish never",
    "sign:win-x64": "pnpm clean && electron-builder --win --x64 --arm64=false --publish never",
    "sign:win-arm64": "electron-builder --win --arm64 --x64=false --publish never",
    "sign:win": "pnpm sign:win-x64 && pnpm sign:win-arm64",
    "test:win-signing": "pnpm clean && electron-builder --win --x64 --publish never",
    "test:signing": "node scripts/test-signing.js",
    "notarize:mac:x64": "NODE_ENV=production CSC_IDENTITY_AUTO_DISCOVERY=false pnpm clean && APPLE_TEAM_ID=A93Q7MKECL electron-builder --mac --x64 --arm64=false",
    "notarize:mac:arm64": "NODE_ENV=production CSC_IDENTITY_AUTO_DISCOVERY=false mv dist dist-x64 && APPLE_TEAM_ID=A93Q7MKECL electron-builder --mac --arm64 --x64=false",
    "notarize:mac:universal": "NODE_ENV=production CSC_IDENTITY_AUTO_DISCOVERY=false mv dist dist-arm64 && APPLE_TEAM_ID=A93Q7MKECL electron-builder --mac --universal",
    "notarize:mac:all": "pnpm notarize:mac:x64 && pnpm notarize:mac:arm64 && pnpm notarize:mac:universal && mkdir -p dist-all && mv dist/* dist-all/ && mv dist-x64/* dist-all/ && mv dist-arm64/* dist-all/ && rm -rf dist && mv dist-all dist",
    "release:mac:x64": "pnpm clean && pnpm version patch && NODE_ENV=production APPLE_TEAM_ID=A93Q7MKECL electron-builder --mac --x64 --arm64=false --publish always",
    "release:mac:arm64": "NODE_ENV=production APPLE_TEAM_ID=A93Q7MKECL electron-builder --mac --arm64 --x64=false --publish always",
    "release:mac:universal": "NODE_ENV=production APPLE_TEAM_ID=A93Q7MKECL electron-builder --mac --universal --publish always",
    "release:mac": "pnpm release:mac:x64 && pnpm release:mac:arm64 && pnpm release:mac:universal",
    "release:win:x64": "pnpm clean && pnpm version patch && electron-builder --win --x64 --publish always",
    "release:win:arm64": "electron-builder --win --arm64 --publish always",
    "release:win": "pnpm release:win:x64 && pnpm release:win:arm64",
    "release:linux:x64": "pnpm clean && pnpm version patch && electron-builder --linux --x64 --publish always",
    "release:linux:arm64": "electron-builder --linux --arm64 --publish always",
    "release:linux": "pnpm release:linux:x64 && pnpm release:linux:arm64",
    "release": "pnpm release:mac && pnpm release:win && pnpm release:linux",
    "super-release": "node scripts/release.js",
    "release:major": "node scripts/release.js major",
    "release:minor": "node scripts/release.js minor",
    "release:patch": "node scripts/release.js patch",
    "release:none": "node scripts/release.js none"
  },
  "keywords": [
    "app",
    "electron",
    "unifi",
    "protect",
    "viewer"
  ],
  "author": "JD Lien <jd@jdlien.com>",
  "license": "",
  "packageManager": "pnpm@10.23.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller"
    ]
  },
  "devDependencies": {
    "@electron/fuses": "^2.0.0",
    "@electron/packager": "^19.0.5",
    "archiver": "^7.0.1",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "electron-reloader": "^1.2.3",
    "glob": "^13.0.6",
    "otplib": "^13.3.0",
    "prettier": "^3.8.1",
    "readline-sync": "^1.4.10",
    "semver": "^7.7.4"
  },
  "dependencies": {
    "dotenv": "^17.3.1",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.8.3"
  }
}
